<Window x:Class="GeofizApp.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        Title="Контур скважины" Height="500" Width="600">
    <Grid Margin="10">
        <lvc:CartesianChart Name="WellChart" Margin="0,0,0,201">
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="X (м)" />
            </lvc:CartesianChart.AxisX>
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Y (м)" />
            </lvc:CartesianChart.AxisY>
        </lvc:CartesianChart>
        
        <lvc:CartesianChart Name="DepthChart" Margin="0,262,0,0">
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="Глубина (м)" />
            </lvc:CartesianChart.AxisX>
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Значение измерения" />
            </lvc:CartesianChart.AxisY>
        </lvc:CartesianChart>

    </Grid>
</Window>
